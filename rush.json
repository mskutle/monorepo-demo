{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.34.3",
  "pnpmVersion": "5.8.0",
  "pnpmOptions": {
    "strictPeerDependencies": false
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0",
  "ensureConsistentVersions": true,
  "repository": {
    "url": "https://github.com/mskutle/monorepo-demo.git"
  },
  "projects": [
    {
      "packageName": "min-side",
      "projectFolder": "apps/min-side"
    },
    {
      "packageName": "milescreate-component-library",
      "projectFolder": "packages/component-library",
      "shouldPublish": true
    },
    {
      "packageName": "min-side-api",
      "projectFolder": "apps/min-side-api"
    },
    {
      "packageName": "milescreate-utils",
      "projectFolder": "packages/utils",
      "shouldPublish": true
    }
  ]
}
